@model GD.FinishingSystem.Entities.RuloProcess

<div>
    <h4>Rulo Process information</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-4">
            <i class="lni lni-line-dashed"></i>
        </dt>
        <dd class="col-sm-6">
            @Html.DisplayFor(model => model.RuloProcessID)
        </dd>
        <dt class="col-sm-4">
            @Html.DisplayNameFor(model => model.BeginningDate)
        </dt>
        <dd class="col-sm-6">
            @Html.Raw(Model.BeginningDate.ToString("yyyy-MM-dd HH:mm"))
        </dd>
        <dt class="col-sm-4">
            @Html.DisplayNameFor(model => model.EndDate)
        </dt>
        <dd class="col-sm-6">
            @Html.Raw(Model.EndDate?.ToString("yyyy-MM-dd HH:mm"))
        </dd>
        <dt class="col-sm-4">
            @Html.DisplayNameFor(model => model.FinishMeter)
        </dt>
        <dd class="col-sm-6">
            @Html.DisplayFor(model => model.FinishMeter)
        </dd>
        <dt class="col-sm-4">
            @Html.DisplayNameFor(model => model.IsFinished)
        </dt>
        <dd class="col-sm-6">
            @Html.Raw(Model.IsFinished ? "YES" : "NO")
        </dd>

    </dl>
</div>
